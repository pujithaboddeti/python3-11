#way 2
n=int(input('enter list1'))
l=list(map(int,input().split()))
n2=int(input('enter list2'))
l2=list(map(int,input().split()))
l.extend(l2)
print(l)
maxx=l[0]
for i in l:
     if i>maxx:
         maxx=i
print(maxx)
